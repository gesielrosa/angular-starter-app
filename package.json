{
  "name": "my-app",
  "version": "0.0.0",
  "scripts": {
    "config": "ts-node setup-env.ts",
    "ng": "ng",
    "start": "npm run config && NG_PERSISTENT_BUILD_CACHE=1 ng serve",
    "start:ssr": "npm run config && ng run my-app:serve-ssr",
    "build": "npm run config && NG_PERSISTENT_BUILD_CACHE=1 ng build",
    "build:ssr": "npm run build && ng run my-app:server",
    "serve:ssr": "node dist/my-app/server/main.js",
    "test": "npm run config && ng test",
    "lint": "ng lint",
    "prettier": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.html,.scss}\"",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.3",
    "@angular/common": "~13.1.3",
    "@angular/compiler": "~13.1.3",
    "@angular/core": "~13.1.3",
    "@angular/forms": "~13.1.3",
    "@angular/platform-browser": "~13.1.3",
    "@angular/platform-browser-dynamic": "~13.1.3",
    "@angular/platform-server": "~13.1.3",
    "@angular/router": "~13.1.3",
    "@nguniversal/express-engine": "^13.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.2",
    "globalthis": "^1.0.2",
    "ngx-cookie": "^5.0.2",
    "ngx-cookie-backend": "^5.0.2",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.4",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "^13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.3",
    "@nguniversal/builders": "^13.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^12.20.42",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "dotenv": "^14.3.0",
    "eslint": "^8.2.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
